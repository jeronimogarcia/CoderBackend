<div>
  <h1 class="text-center mt-5 mb-5">Cart</h1>
  <div class="w-75 m-auto">
    <table class="table table-sm table-bordered table-dark w-60%">
      <thead>
        <tr>
          <th scope="col">Producto</th>
          <th scope="col">Precio</th>
          <th scope="col">Quantity</th>
        </tr>
      </thead>
      <tbody>
        {{#each products}}
          <tr>
            <td>{{product.title}}</td>
            <td>${{product.price}}</td>
            <td>{{quantity}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>